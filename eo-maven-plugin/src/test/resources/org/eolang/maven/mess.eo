+package org.eolang.examples
+alias stdout org.eolang.io.stdout

3.14 > pi

[n] > alpha
  [n+] > x
    a pi 666
      n+
      two.f1.f2 x
  x n:n+
  stdout
    sprintf > f!
      "Hello, "
      x 0x1fff n

alpha > beta
  alpha
    'Z'




[n+] > alpha$f!
  sprintf
    "Hello, "
    alpha$x 0x1fff n+

[n] > alpha
  alpha$x n:n+
  stdout
    alpha$f n:n+

alpha > beta
  alpha
    'Z'
