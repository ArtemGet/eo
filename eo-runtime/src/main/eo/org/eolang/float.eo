+package org.eolang

[] > float
  # Equals to another number
  [x] > eq /bool

  # Multiplication of $ and x
  [x] > mul /float

  # Sum of $ and x
  [x] > add /float

  # Negation of $
  [] > neg /float

  # Difference between $ and x
  [x] > sub
    ^.add (x.neg) > @

  # x'th power of $
  [x] > pow /float
