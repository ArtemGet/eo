+package org.eolang
+alias org.eolang.io.stdout
+alias org.eolang.txt.sprintf
+junit

[] > identical-objects
  42 > a
  42 > b
  identical a b > @

[] > identical-composite-objects
  [x] > foo
  foo 42 > a
  a > b
  identical a b > @

[] > two-similar-but-not-identical-objects
  20.add 22 > a
  42 > b
  not. > @
    identical a b

[] > different-objects
  7 > a
  42 > b
  not. > @
    identical a b

[] > different-types
  42.0 > a
  42 > b
  not. > @
    identical a b

[] > identical-abstract-objects
  [] > a
    "hello" > @
  a > b
  identical a b > @

[] > different-abstract-and-copy-objects
  [x] > a
    "hello" > @
  a 5 > b
  not. > @
    identical a b
