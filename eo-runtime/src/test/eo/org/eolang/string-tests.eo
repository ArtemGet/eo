+package org.eolang
+junit

[] > matches-against-regular-expression
  (/([a-z])+/.matches "dude") > @

[] > trims-string
  eq. > @
    "Hello, dude"
    "Hello, dude  ".trim

[] > turns-string-into-int
  eq. > @
    42
    "42".as-int

[] > turns-string-into-float
  eq. > @
    3.1415926
    "3.1415926".as-float

[] > picks-a-single-char
  eq. > @
    'e'
    "Hello, world!".char-at 1

[] > reads-the-length
  eq. > @
    12
    "Hello, друг!".length

